{
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:json/recommended-with-comments",
    "plugin:@typescript-eslint/recommended",
    "plugin:jest/all"
  ],
  "plugins": ["json", "@typescript-eslint", "jest"],
  "parser": "@typescript-eslint/parser",
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "registerPlugin": true,
    "ui": true,
    "map": true,
    "park": true,
    "context": true
  }
}

{
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:json/recommended-with-comments",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["prettier", "json", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "registerPlugin": true,
    "ui": true,
    "map": true,
    "park": true,
    "context": true
  }
}
